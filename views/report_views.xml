<odoo>
    <data>
        <record id="view_rental_report" model="ir.ui.view">
            <field name="name">rental.report.list</field>
            <field name="model">rental.report</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                    <field name="is_default"/>
                    <field name="sequence"/>
                </list>
            </field>
        </record>
        <record id="action_rental_report" model="ir.actions.act_window">
            <field name="name">Reportes</field>
            <field name="res_model">rental.report</field>
            <field name="view_mode">list</field>
        </record>
    </data>
</odoo>
